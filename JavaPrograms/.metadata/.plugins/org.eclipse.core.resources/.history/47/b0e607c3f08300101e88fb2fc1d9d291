package constructor;

class Student{
	String name;
	int age;
	
	Student(String name, int age){
		this.name = name;
		this.age = age;
	}
	void display() {
		System.out.println("name "+name+" age "+age);
	}
}
public class Constructor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       Student s = new Student("Teja",22);
       s.display();
	}

}
