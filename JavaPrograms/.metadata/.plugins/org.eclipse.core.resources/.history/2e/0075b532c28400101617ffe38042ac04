package arrays;

public class ProblemsOnArrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
          int [] nums = {23,4,56,75,98,90};
          int sum = 0;
          for(int i=0; i<nums.length; i++) {
        	  sum = sum + nums[i];
          }
          System.out.println("Sum of the elements of array"+ sum);
	}

}
